
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  rut       String  @unique
  password  String
  nombre    String? // Opcional
  apellido  String? // Opcional
  email     String? @unique // Opcional y Ãºnico
  role      String  @default("USER")
}




